{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "src/shared/layouts/landing/ui.jsx",
        "src/shared/layouts/LandingBigLayout/layout.jsx",
        "src/entities/navBarButton/navBarButton.jsx",
        "src/features/Landing/navBarDesktop/navBar.tsx",
        "src/shared/Landing/toolsSection/tools.jsx"
    ],
    "exclude": ["node_modules"]
}
